{
  "examples": {
    "1_health_check": {
      "description": "Check if server is running",
      "method": "GET",
      "url": "http://localhost:8080/health",
      "command": "curl http://localhost:8080/health"
    },
    "2_initialize": {
      "description": "Initialize MCP protocol",
      "method": "POST",
      "url": "http://localhost:8080/mcp",
      "body": {
        "jsonrpc": "2.0",
        "id": 1,
        "method": "initialize",
        "params": {
          "protocolVersion": "2024-11-05",
          "capabilities": {},
          "clientInfo": {
            "name": "test-client",
            "version": "1.0.0"
          }
        }
      },
      "command": "curl -X POST http://localhost:8080/mcp -H 'Content-Type: application/json' -d '{\"jsonrpc\":\"2.0\",\"id\":1,\"method\":\"initialize\",\"params\":{}}'"
    },
    "3_list_tools": {
      "description": "List all available MCP tools",
      "method": "POST",
      "url": "http://localhost:8080/mcp",
      "body": {
        "jsonrpc": "2.0",
        "id": 2,
        "method": "tools/list",
        "params": {}
      },
      "command": "curl -X POST http://localhost:8080/mcp -H 'Content-Type: application/json' -d '{\"jsonrpc\":\"2.0\",\"id\":2,\"method\":\"tools/list\",\"params\":{}}'"
    },
    "4_list_resources": {
      "description": "List all available MCP resources",
      "method": "POST",
      "url": "http://localhost:8080/mcp",
      "body": {
        "jsonrpc": "2.0",
        "id": 3,
        "method": "resources/list",
        "params": {}
      }
    },
    "5_list_prompts": {
      "description": "List all available MCP prompts",
      "method": "POST",
      "url": "http://localhost:8080/mcp",
      "body": {
        "jsonrpc": "2.0",
        "id": 4,
        "method": "prompts/list",
        "params": {}
      }
    },
    "6_call_analyze_file": {
      "description": "Test analyze_file tool - Replace YOUR_FILE_KEY with actual Figma file key",
      "method": "POST",
      "url": "http://localhost:8080/mcp",
      "body": {
        "jsonrpc": "2.0",
        "id": 5,
        "method": "tools/call",
        "params": {
          "name": "analyze_file",
          "arguments": {
            "fileKey": "YOUR_FILE_KEY"
          }
        }
      },
      "command": "curl -X POST http://localhost:8080/mcp -H 'Content-Type: application/json' -d '{\"jsonrpc\":\"2.0\",\"id\":5,\"method\":\"tools/call\",\"params\":{\"name\":\"analyze_file\",\"arguments\":{\"fileKey\":\"YOUR_FILE_KEY\"}}}'"
    },
    "7_call_list_components": {
      "description": "Test list_components tool - Replace YOUR_FILE_KEY with actual Figma file key",
      "method": "POST",
      "url": "http://localhost:8080/mcp",
      "body": {
        "jsonrpc": "2.0",
        "id": 6,
        "method": "tools/call",
        "params": {
          "name": "list_components",
          "arguments": {
            "fileKey": "YOUR_FILE_KEY"
          }
        }
      }
    },
    "8_read_resource": {
      "description": "Read a Figma file resource - Replace YOUR_FILE_KEY with actual Figma file key",
      "method": "POST",
      "url": "http://localhost:8080/mcp",
      "body": {
        "jsonrpc": "2.0",
        "id": 7,
        "method": "resources/read",
        "params": {
          "uri": "figma://file/YOUR_FILE_KEY"
        }
      }
    }
  }
}


